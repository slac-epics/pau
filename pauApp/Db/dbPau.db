record(ai, "$(PAU_REC):FIDCNT")
{
    field(DTYP, "PAU")
    field(SCAN, "Passive")
    field(INP, "@getFidCnt $(PAU)")
    field(DESC, "fiducial cnt for $(PAU)")
    field(EGU, "Counts")
    field(PREC, "0")
}

record(calc, "$(PAU_REC):FIDRATE")
{
    field(SCAN, "2 second")
    field(CALC, "(C-B)<0.?A:(C-B)/2.")
    field(INPA, "$(PAU_REC):FIDRATE.VAL NPP NMS")
    field(INPB, "$(PAU_REC):FIDCNT.VAL NPP NMS")
    field(INPC, "$(PAU_REC):FIDCNT.VAL PP NMS")
    field(DESC, "fiducial rate for $(PAU)")
    field(PREC, "2")
}

