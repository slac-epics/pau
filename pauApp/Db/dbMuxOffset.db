## fanout ##

record(fanout, "$(MUX_REC):FANOUT0")
{
    field(DESC, "MUX fanout 0")
    field(LNK1, "$(MUX_REC):CALCOUT_0")
    field(LNK2, "$(MUX_REC):CALCOUT_1")
    field(LNK3, "$(MUX_REC):CALCOUT_2")
    field(LNK4, "$(MUX_REC):CALCOUT_3")
    field(FLNK, "$(MUX_REC):FANOUT1")
}

record(fanout, "$(MUX_REC):FANOUT1")
{
    field(DESC, "MUX fanout 1")
}



