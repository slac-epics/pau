record(ao, "$(MUX_REC)")
{
    field(DTYP, "Soft Channel")
    field(DESC, "Master SetValue")
    field(FLNK, "$(MUX_REC):FANOUT0")
    field(PREC, "3")
    field(PINI, "YES")
    info(autosaveFields, "VAL")		
}

## data slot 0 ##

record(ao, "$(MUX_REC):OFFSET_0")
{
    field(DTYP, "Soft Channel")
    field(DESC, "Offset for DS0")
    field(PREC, "3")
    field(FLNK, "$(MUX_REC):CALCOUT_0")
    field(PINI, "YES")
    info(autosaveFields, "VAL")		
}

record(calcout, "$(MUX_REC):CALCOUT_0")
{
    field(DESC, "Calcout for DS0")
    field(CALC, "A+B")
    field(INPA, "$(MUX_REC) NPP NMS")
    field(INPB, "$(MUX_REC):OFFSET_0 NPP NMS")
    field(OUT, "$(MUX_REC):SETDATA_0 PP NMS")
}

